  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.1",
    "@mdx-js/react": "^1.6.1",
    "babel-preset-gatsby": "^0.4.4",
    "bulma": "^0.8.2",
    "bulma-o-steps": "^1.0.9",
    "gatsby": "2.22.0",
    "gatsby-image": "^2.4.4",
    "gatsby-plugin-google-tagmanager": "^2.3.1",
    "gatsby-plugin-mdx": "^1.2.9",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-sharp": "^2.6.6",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-remark-copy-linked-files": "^2.3.2",
    "gatsby-remark-embed-snippet": "^4.3.1",
    "gatsby-remark-images": "^3.1.2",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-source-filesystem": "^2.3.5",
    "gatsby-theme-bulmaio": "*",
    "gatsby-transformer-remark": "^2.8.10",
    "gatsby-transformer-sharp": "^2.5.2",
    "gatsby-transformer-yaml": "^2.4.1",
    "identity-obj-proxy": "^3.0.0",
    "prismjs": "^1.20.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gif-player": "^0.4.2",
    "react-helmet": "^5.2.1",
    "react-scroll": "^1.7.16",
    "remark": "^12.0.0",
    "remark-external-links": "^6.0.0",
    "remark-html": "^11.0.2",
    "sharp": "^0.25.3",
    "video.js": "^7.6.5",
    "videojs-youtube": "^2.6.0"
  },
  "devDependencies": {
    "@pacote/eslint-config-jest": "^1.0.5",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.3",
    "@types/qs": "^6.9.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-scroll": "^1.5.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/sharp": "^0.25.0",
    "@types/video.js": "^7.3.9",
    "babel-jest": "^26.0.1",
    "eslint-plugin-testing-library": "^3.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "node-sass": "^4.14.1",
    "react-test-renderer": "^16.13.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setup-jest.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "peerDependencies": {}
}
