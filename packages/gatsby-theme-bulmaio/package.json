  "license": "MIT",
  "scripts": {
    "sass": "node-sass src/components/layout/index.scss -o build/css/",
    "build": "tsc -w"
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/lunr": "^2.3.4",
    "babel-preset-gatsby": "^1.8.0",
    "bulma": "^0.9.3",
    "bulma-o-steps": "^1.1.0",
    "gatsby": "^3.8.0",
    "gatsby-plugin-feed": "^3.8.0",
