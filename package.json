{
  "name": "gatsyby-typescript-template",
  "description": "gatsyby with typescript and tslint",
  "version": "1.0.0",
  "author": "Ryo Ikeura ( ikeryo1182@gmail.com )",
  "keywords": [
    "gatsby, typescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "fix": "prettier --write 'src/**/*.{ts,tsx,scss,md}' && tslint --fix 'src/**/*.{ts,tsx}' -p tsconfig.json && stylelint --fix 'src/**/*.scss' "
