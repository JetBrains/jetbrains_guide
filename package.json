    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.5",
    "gatsby-transformer-sharp": "^2.1.5",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-test-renderer": "^16.6.0",
    "remark": "^10.0.0",
    "remark-html": "^8.0.0",
