{
  "name": "pycharm_guide",
  "description": "Gatsby-driven compendium of PyCharm learning resources",
  "version": "1.0.0",
  "author": "Paul Everitt <paul.everitt@jetbrains.com>",
  "keywords": [
    "gatsby, typescript"
  ],
  "license": "Apache-2.0",
  "main": "n/a",
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "build": "gatsby build --prefix-paths",
    "start": "gatsby develop",
    "serve": "lite-server",
    "deploy": "firebase deploy"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.0",
    "@types/react": "^16.7.10",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.7",
    "@types/react-scroll": "^1.5.4",
    "@types/react-test-renderer": "^16.0.3",
    "@types/video.js": "^7.2.5",
    "bulma": "^0.7.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "gatsby": "^2.0.59",
    "gatsby-image": "^2.0.22",
    "gatsby-plugin-manifest": "^2.0.11",
    "gatsby-plugin-offline": "^2.0.18",
    "gatsby-plugin-react-helmet": "^3.0.3",
    "gatsby-plugin-sass": "^2.0.5",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-tslint": "0.0.2",
    "gatsby-plugin-typescript": "^2.0.2",
    "gatsby-remark-embed-snippet": "^3.1.2",
    "gatsby-remark-prismjs": "^3.1.2",
    "gatsby-source-filesystem": "^2.0.10",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.9",
    "jest": "^23.6.0",
    "lite-server": "^2.4.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.15.3",
    "prismjs": "^1.15.0",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-scroll": "^1.7.10",
    "react-test-renderer": "^16.6.0",
    "remark": "^10.0.1",
    "remark-html": "^9.0.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "moduleNameMapper": {
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/setupTests.ts"
    ]
  },
  "dependencies": {
    "bulma-steps-component": "^0.5.3",
    "gatsby-remark-copy-linked-files": "^2.0.6",
    "gatsby-remark-images": "^2.0.4",
    "video.js": "^7.2.4",
    "videojs-youtube": "^2.6.0"
  }
}
